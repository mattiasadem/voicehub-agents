{
  "id": "reach-3",
  "name": "Reach-3 (SDR Bot)",
  "description": "Automated SDR - cold emails, follow-ups, demo booking",
  "model": "kimi",
  "schedule": "0 9,14 * * 1-5",
  "timezone": "Europe/Berlin",
  "timeout": 300,
  "enabled": false,
  "inputs": {
    "lead_source": "scout-alpha/output",
    "sequence": "3-touch",
    "personalization": true
  },
  "actions": [
    "draft_cold_email",
    "follow_up_sequence", 
    "book_demo",
    "update_crm"
  ],
  "output": "/data/.openclaw/workspace/voicehub-agents/memory/reach-3/",
  "limits": {
    "emails_per_day": 50,
    "follow_ups_per_day": 100
  }
}
