{
  "id": "intel-9",
  "name": "Competitive Intelligence Agent",
  "description": "Monitors competitors, tracks pricing/features, generates battlecards",
  "model": "kimi",
  "schedule": "0 9 * * *",
  "timezone": "Europe/Berlin",
  "timeout": 300,
  "enabled": false,
  "competitors": [
    "smith.ai",
    "nexa.ai",
    "olo.com",
    "synthetix.com"
  ],
  "tracking": {
    "pricing_pages": true,
    "feature_pages": true,
    "blog_posts": true,
    "press_releases": true
  },
  "alerts": {
    "pricing_change": { "priority": "high", "notify": ["founder", "sales"] },
    "new_feature": { "priority": "medium", "notify": ["founder", "product"] },
    "funding_news": { "priority": "high", "notify": ["founder"] }
  },
  "outputs": {
    "battlecards": "memory/intel-9/battlecards/",
    "alerts": "memory/intel-9/alerts/",
    "weekly_digest": "memory/intel-9/digests/"
  },
  "battlecard_template": [
    "pricing_comparison",
    "feature_matrix",
    "win_loss_talking_points",
    "objection_handling"
  ]
}
